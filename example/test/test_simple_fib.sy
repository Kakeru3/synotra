task main() distributed {
    val n = 10
    fibTasks(n)
}

task fibTasks(count: Int) distributed {
    fun fib(x: Int): Int {
        if x <= 1 {
            return x
        } else {
            return fib(x - 1) + fib(x - 2)
        }
    }

    for i in 1..count do {
        val result = fib(i)
        print(result)
    }
}
