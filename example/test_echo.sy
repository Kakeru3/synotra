class Start()

actor main(name: String) {
    io fun print(msg: String) { }

    io fun run(msg: Start) {
        var result = ask("Worker1", "echo", 35)
        print("Echo result: ")
        print(result)
    }
}

actor Worker1(name: String) {
    io fun run(msg: Start) { }
    io fun echo(n: Int) {
        return n
    }
}
