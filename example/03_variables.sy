// Variables and String Interpolation

actor main(name: String) {
    io fun run() {
        var greeting = "Hello"
        var target = "World"
        println("${greeting}, ${target}!")
        
        var x = 10
        var y = 20
        println("x = ${x}, y = ${y}")
        println("Sum: ${x + y}")

        var z: Int
        z = 30
        println("z = ${z}")
    }
}
