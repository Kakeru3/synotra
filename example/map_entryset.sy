actor main(env: Env) {
    io fun run() {
        var map = MutableMap.new()
        map.put("a", 10)
        map.put("b", 20)
        map.put("c", 30)

        println("Map size: ${map.size()}")

        println("Iterating over entrySet:")
        for (entry in map.entrySet()) {
            println("Key: ${entry.key()}, Value: ${entry.value()}")
        }
    }
}
