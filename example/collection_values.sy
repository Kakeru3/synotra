actor main(env: Env) {
  io fun run() {
    var map = MutableMap.new()
    map.put("a", 10)
    map.put("b", 20)
    map.put("c", 30)

    println("Map size: ${map.size()}")
    
    var values = map.values()
    println("Values size: ${values.size()}")

    var i = 0
    while (i < values.size()) {
      println("Value ${i}: ${values.get(i)}")
      i = i + 1
    }
  }
}
