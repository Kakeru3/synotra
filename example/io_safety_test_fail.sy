class Start()

actor main(name: String) {// ❌ これはエラーになるはず：non-IO関数からIO関数を呼び出せない
    fun pure_function(n: Int): Int {
        print("This should cause an error!")  // IO関数をpure関数から呼び出し
        return n
    }

    // ❌ io fun は戻り値を持てないのでエラーになります
    io fun io_function(n: Int): Int {
        var calc = n + 10
        return calc // 戻り値を持てない
    }

    io fun run(msg: Start) {
        var pure_result = pure_function(10)
        var io_result = io_function(10)
        print(pure_result)
        print(io_result)
    }
}
